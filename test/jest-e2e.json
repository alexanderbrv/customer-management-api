{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^@src/(.*)$": "<rootDir>/../src/$1",
    "^@common/(.*)$": "<rootDir>/../src/common/$1",
    "^@generated/(.*)$": "<rootDir>/../generated/$1"
  },
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/setup-e2e-env.ts"]
}
